
generator client {
  provider = "prisma-client-js"
  previewFeatures = ["reactNative"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DATABASE_URL_UNPOOLED")
}
model User {
  id           Int     @id @default(autoincrement())
  name         String
  email        String  @unique
  password     String
  picture      String?
  subscription String?
  credits      Int?
  gender       String?
  goal         String?
  weight       String?
  height       String?
  age          String?
  calories     String?
  Proteins     String?
}


model UserMeal {
  id        Int     @id @default(autoincrement())
  email     String
  recepies  Json
  createdAt DateTime @default(now())
}
